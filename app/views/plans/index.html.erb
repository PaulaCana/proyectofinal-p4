<p id="notice"><%= notice %></p>






<h1>Plans</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Localization</th>
      <th>Phone</th>
      <th>People</th>
      <th>Private</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @plans.each do |plan| %>
      <tr>
        <td><%= plan.title %></td>
        <td><%= plan.localization %></td>
        <td><%= plan.phone %></td>
        <td><%= plan.people %></td>
        <td><%= plan.private %></td>
        <td><%= link_to 'Show', plan %></td>
        <td><%= link_to 'Edit', edit_plan_path(plan) %></td>
        <td><%= link_to 'Destroy', plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Plan', new_plan_path %>
